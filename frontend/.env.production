# Production Environment Variables for Banking System Frontend

# API Configuration
REACT_APP_API_URL=https://banking-backend.onrender.com/api

# App Configuration
REACT_APP_NAME=Banking System
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Security Configuration
REACT_APP_ENABLE_SECURITY_LOGS=false
REACT_APP_SESSION_TIMEOUT_MINUTES=30

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=false